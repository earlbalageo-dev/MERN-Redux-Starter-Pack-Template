{
  "name": "mern-redux-template",
  "version": "1.0.0",
  "description": "a starter pack for mern stack with redux",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "seed": "node backend/seeder/seeder",
    "seed-destroy": "node backend/seeder/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earlbalageo-dev/MERN-Redux-Starter-Pack-Template.git"
  },
  "keywords": [
    "MongoDb",
    "Express.js",
    "REACT",
    "Node.js",
    "Boiler",
    "boiler-plate",
    "template"
  ],
  "author": "Earl Balageo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/earlbalageo-dev/MERN-Redux-Starter-Pack-Template/issues"
  },
  "homepage": "https://github.com/earlbalageo-dev/MERN-Redux-Starter-Pack-Template#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.14",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15"
  }
